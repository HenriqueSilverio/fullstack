[{
  "group": "admin",
  "permissions": [{
    "resource": "*",
    "methods": "*",
    "action": "allow"
  }]
}, {
  "group": "user",
  "permissions": [{
    "resource": "users/",
    "methods": [ "GET" ],
    "action": "deny"
  }, {
    "resource": "users/:id",
    "methods": [ "GET", "PUT" ],
    "action": "allow"
  }]
}]
